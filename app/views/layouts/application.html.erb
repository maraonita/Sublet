<!DOCTYPE html>
<html>
<head>
  <title>SubIt</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
  <%= stylesheet_link_tag 'application', 'http://fonts.googleapis.com/css?family=Lato:300,400,700,900', media: 'all', 'data-turbolinks-track' => true %>

</head>
<body>

	<%= render 'layouts/header' %>

	<%if false %> 
	<!--STILL UNSURE HOW TO HANDLE ERROR REPORTING WITH THIS-->
		<div class="container">

			<p class="notice"><%= notice %></p>
		    <p class="alert"><%= alert %></p>

		</div>
	<% end %>
	<%= yield %>
	<%= render 'layouts/footer'%>
	

</body>
</html>
